{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "plugin:node/recommended",
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
